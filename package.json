{
  "name": "duff",
  "version": "1.1.3",
  "description": "Aplicacao para indicacao de melhor estilo de cerveja e playlist para acompanhamento, de acordo com temperatura desejada",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node ./dist/main",
    "dev": "npm run build && ts-node ./src/main",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "lint": "prettier -w . && eslint . --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:staged": "npm test -- --findRelatedTests",
    "test:watch": "npm test -- --watch",
    "test:cover": "npm test -- --coverage",
    "pretest:integ": "npm run pgtest:up",
    "test:integ": "npm test -- -c ./jest.integration.config.js --coverage",
    "posttest:integ": "npm run pgtest:down",
    "test:all": "npm run test:cover && npm run test:integ",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate -- -d ./src/infra/database/pg-datasource.ts ./src/infra/database/migrations/%npm_config_migrationName%",
    "migration:create": "npm run typeorm migration:create ./src/infra/database/migrations/%npm_config_migrationName%",
    "migration:up": "npm run typeorm migration:run -- -d ./src/infra/database/pg-datasource.ts",
    "migration:down": "npm run typeorm migration:revert -- -d ./src/infra/database/pg-datasource.ts",
    "docker:build": "docker build -t duff . ",
    "pg:up": "docker-compose -f docker-compose.postgres.yml up -d",
    "postpg:up": "npm run migration:up",
    "pg:down": "docker-compose -f docker-compose.postgres.yml down",
    "pgtest:up": "docker-compose -f docker-compose.postgres.tests.yml up -d",
    "pgtest:down": "docker-compose -f docker-compose.postgres.tests.yml down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/joi": "^17.2.3",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.11.13",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "git-commit-msg-linter": "^4.4.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "pg-mem": "^2.6.4",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "module-alias": "^2.2.2",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0"
  }
}
